    <style>
        .featurette-divider {
            margin: 1rem 0;
        }
    </style>

</head>

<body>
    <header class="p-3 bg-dark text-white fixed-top">
        <div class="container">
            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                    <svg class="bi me-2" width="40" height="32" role="img" style="fill: #ff8800;">
                        <use xlink:href="#icon" />
                    </svg>
                </a>

                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                    <li><a href="/" class="nav-link px-2 text-white">Inicio</a></li>
                    <li><a href="/ui/incidents" class="nav-link px-2 text-white">Incidencias</a></li>
                    <li><a href="/ui/incidents/inaccessible_accessibility_features"
                            class="nav-link px-2 text-secondary">Medidas de accesibilidad</a></li>
                    <li><a href="/ui/incidents/upload_csv" class="nav-link px-2 text-white">Cargar CSV</a></li>
                    <li><a href="/faqs" class="nav-link px-2 text-white">FAQs</a></li>
                    <li><a href="/nosotros" class="nav-link px-2 text-white">Nosotros</a></li>
                    <li><a href="/contacto" class="nav-link px-2 text-white">Contacto</a></li>
                </ul>


                <div class="text-end">
                    <button type="button" class="btn btn-outline-light me-2">Ingresar</button>
                    <button type="button" class="btn btn-warning">Registrarse</button>
                </div>
            </div>
        </div>
    </header>

    <div class="container mt-3">
        <div class="row justify-content-between pt-5">
            <div class="col-15 pt-3 d-flex justify-content-between" style="padding-top: 2rem !important;">
                <h2 style="display: inline;">Medidas de Accesibilidad Inaccesibles</h2>
            </div>
            <link rel="stylesheet"
                href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css">
            <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css">

            <hr class="featurette-divider">

            <table id="tablax" class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">Código de catalogo</th>
                        <th scope="col">Tipo</th>
                        <th scope="col">Estado</th>
                        <th scope="col">Línea</th>
                        <th scope="col">Estación</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each inaccessibleAccessibilityFeatures}}
                    <tr>
                        <td>{{this.catalogCode}}</td>
                        <td>{{this.type}}</td>
                        <td>{{this.status}}</td>
                        <td>{{this.station.line}}</td>
                        <td>{{this.station.name}}</td>
                    </tr>
                    {{/each}}

                    <body>
                        <script src="https://code.jquery.com/jquery-3.4.1.js"
                            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous">
                            </script>
                        <!-- DATATABLES -->
                        <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js">
                        </script>
                        <!-- BOOTSTRAP -->
                        <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js">
                        </script>

                        <script>
                            document.addEventListener('DOMContentLoaded', function () {
                                function traducirValores() {
                                    const traduccionesType = {
                                        'ELEVATOR': 'Ascensor',
                                        'ESCALATOR': 'Escalera mecánica',
                                        'SIGNAGE': 'Señalización',
                                        'RAMP': 'Rampa'
                                    };

                                    const traduccionesStatus = {
                                        'FUNCTIONAL': 'Funcional',
                                        'INACCESSIBLE': 'Inaccesible'
                                    };
                                    const celdasType = document.querySelectorAll('#tablax tbody td:nth-child(2)');
                                    const celdasStatus = document.querySelectorAll('#tablax tbody td:nth-child(3)');

                                    celdasType.forEach(function (celda) {
                                        const valorOriginal = celda.textContent.trim();
                                        if (traduccionesType[valorOriginal]) {
                                            celda.textContent = traduccionesType[valorOriginal];
                                        }
                                    });

                                    celdasStatus.forEach(function (celda) {
                                        const valorOriginal = celda.textContent.trim();
                                        if (traduccionesStatus[valorOriginal]) {
                                            celda.textContent = traduccionesStatus[valorOriginal];
                                        }
                                    });
                                }

                                traducirValores();

                                $('#tablax').DataTable();
                            });
                        </script>
                    </body>
                </tbody>
            </table>
        </div>
    </div>
</body>

</html>